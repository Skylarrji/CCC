apple1 = int(input())
apple2 = int(input())
apple3 = int(input())

applescore = 3*apple1 + 2*apple2 + apple3

banana1 = int(input())
banana2 = int(input())
banana3 = int(input())

bananascore = 3*banana1 + 2*banana2 + banana3

if applescore > bananascore:
  print("A")

elif bananascore > applescore:
  print("B")

else:
  print("T")
