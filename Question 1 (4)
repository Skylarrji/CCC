smalltreats = int(input())
mediumtreats = int(input())
largetreats = int(input())

if smalltreats < 0 or mediumtreats < 0 or largetreats < 0:
  print("No negative values.")

else:
  happiness = 1*smalltreats + 2*mediumtreats + 3*largetreats

  if happiness >= 10:
    print("happy")

  else:
    print("sad")